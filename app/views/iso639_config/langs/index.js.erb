$('#listed-langs').html("<%= j render('langs_table', langs: @langs) %>")
<% if !@deleted_lang.blank? -%>
  <% lang = Iso639Config::Lang.new(bib_code: @deleted_lang) -%>
  $('#lang-<%= @lang.alpha3 %>').html("<%= j render('add_to_list_form', lang: lang) %>")
<% else -%>
  $('#lang-<%= @lang.alpha3 %>').html("Listed")
<% end -%>
